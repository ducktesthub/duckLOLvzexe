-- Webhook URL của bạn
local webhookURL = "YOUR_DISCORD_WEBHOOK_URL"

-- Hàm để gửi tin nhắn đến Discord webhook
local function sendToDiscord(message)
    local http = game:GetService("HttpService")
    local data = {
        ["content"] = message
    }
    local jsonData = http:JSONEncode(data)
    
    http:PostAsync(webhookURL, jsonData, Enum.HttpContentType.ApplicationJson)
end

-- Hàm để kiểm tra và nhặt trái ác quỷ
local function collectFruits()
    local fruits = game.Workspace:GetChildren()
    for _, fruit in ipairs(fruits) do
        if fruit:IsA("Tool") and fruit.Name == "Fruit" then
            -- Di chuyển tới trái ác quỷ và nhặt nó
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = fruit.Handle.CFrame
            wait(1)
            fireclickdetector(fruit.ClickDetector)
            
            -- Gửi tin nhắn đến Discord webhook
            sendToDiscord("Đã nhặt được trái ác quỷ: " .. fruit.Name)
        end
    end
end

-- Lặp lại quá trình nhặt trái ác quỷ mỗi 10 giây
while true do
    collectFruits()
    wait(10)
end
